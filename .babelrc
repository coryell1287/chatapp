{
  "sourceMaps": "inline",
  "presets": [
    "react",
    "airbnb",
    "env",
    "es2017",
    "stage-0",
    "stage-2"
  ],
  "plugins": [
    "babel-plugin-transform-class-properties",
    "syntax-async-functions",
    "transform-async-generator-functions",
    "transform-runtime",
    "add-module-exports",
    "transform-regenerator"
  ]
}



